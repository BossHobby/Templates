              ],
              "gyro": [
                "icm42688p"
              ]
            },
            "profile": {
              "motor": {
                "gyro_orientation": 48
              }
            }
          },
          {
            "name": "x12_bmi270",
            "selector": {
              "fc": [
                "x12_elrs"
              ],
              "gyro": [
                "bmi270"
              ]
            },
            "profile": {
              "motor": {
                "gyro_orientation": 36
              }
            }
          },
          {
            "name": "superbee_bmi270",
            "selector": {
              "fc": [
                "superbee_f4_elrs"
              ],
              "gyro": [
                "bmi270"
              ]
            },
            "profile": {
              "motor": {
                "gyro_orientation": 36
              }
            }
          },
          {
            "name": "CrossF4_HDZero Uart",
            "selector": {
              "fc": [
                "cross_f4_elrs"
              ],
              "vtx": [
                "hdzero"
              ]
            },
            "profile": {
              "serial": {
                "hdzero": 1
              }
            }
          }
        ]
      }
    ],
    "category": "setup",
    "profile": "setup/Fractal_65/profile.yaml",
    "image": "setup/Fractal_65/image.jpg"
